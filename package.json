{
  "name": "curso-webpack",
  "version": "1.0.0",
  "description": "Aprendiendo Webpack con Platzi",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./external-css/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-mini-css-extract/webpack.config.js",
    "build:multiple:css": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev:server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-image/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:videos": "webpack --config ./url-loader-videos/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Juan Carlos Jiménez Gutiérrez <jcjimenez29@misena.edu.co> (https://twitter.com/Jce_)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "json5-loader": "^1.0.1",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "nib": "^1.1.2",
    "node-sass": "^4.9.2",
    "postcss": "^7.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.6",
    "rupture": "^0.7.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
